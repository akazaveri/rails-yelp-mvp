<%= simple_form_for(restaurant) do |f| %>
  <strong><%= f.input :name %></strong>
  <strong><%= f.input :address %></strong>
  <strong><%= f.input :phone_number %></strong>
  <strong><%= f.input :category, collection: Restaurant::CATEGORIES %></strong>
  <%= f.submit class: "btn btn-primary"%>
<% end %>
